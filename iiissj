                        command: 'curl "{{URL}}/api/data" -H "Origin: https://attacker.com" -I | grep "Access-Control-Allow-Origin: https://attacker.com"'
                    },
                    {
                        id: 'header004',
                        name: 'Clickjacking Protection Test',
                        severity: 'medium',
                        cve: '',
